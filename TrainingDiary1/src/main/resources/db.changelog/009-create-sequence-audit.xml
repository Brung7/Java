<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        logicalFilePath="db/changelog.xml">
    <changeSet id="009-create-sequence-audit.xml" author="vladimir">
        <preConditions onFail="MARK_RAN">
            <not>
                <sequenceExists sequenceName="auditsequence"/>
            </not>
        </preConditions>

        <createSequence sequenceName="auditsequence" schemaName="useraudit" dataType="BIGINT" minValue="5" incrementBy="3"/>


    </changeSet>

</databaseChangeLog>